<% user.each do |user| %>
  <%= attachment_image_tag user, :profile_image_id %>
  <P>name	 <%= user.name %></p>
  <P>introduction	 <%=user.introduction %></p>
<% end %>
<%= link_to"edit" , edit_user_path%>
<%= form_with model: book, local:true do |f| %>
  <h2>New book</h2>
  <h4>title</h4>
  <%= f.text_field :title %>
  <h4>option</h4>
  <%= f.text_area :body %><br>
  <%= f.submit 'Create' %>
<% end %>