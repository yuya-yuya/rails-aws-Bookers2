<% notice %>
<div class="container">
  <div class="row">
     <div class="col-md-3">
      <%= render 'users/list', user: @user ,book: @book %>
     </div>
     <div class="col-md-9">
       <h2>Users</h2>
        <% @users.each do |user| %>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>image</th>
              <th>name</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <rd></rd>
              <rd><%= user.name%></rd>
              <rd><%= link_to"Show" ,user_path(@user.id)%></rd>
            </tr>
          </tbody>
        </table>
        <% end %>
     </div>
  </div>
</div>


