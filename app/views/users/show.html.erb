<%= notice %>
<div class="container">
  <div class="row">
    <div class="col-lg-4">
      <%= render 'users/list', user: @user ,book: @book %>
    </div>
    <div class="col-lg-8">
      <h2>Books</h2>
      <% @books.each do |book| %>
      <table class="table table-striped">
        <thead>
          <tr>
            <th></th>
            <th>title</th>
            <th>option</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td></td>
            <td><%= book.title%></td>
            <td><%= book.body%></td>
          </tr>
        </tbody>
      </table>
      <% end %>
    </div>
  </div>
</div>